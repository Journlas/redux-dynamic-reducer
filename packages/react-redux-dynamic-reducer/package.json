{
  "name": "react-redux-dynamic-reducer",
  "version": "1.0.0",
  "description": "react-redux Provider extension for redux-subspace",
  "author": "Michael Peyper",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dist": "babel src --out-dir lib --copy-files",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "nyc mocha --compilers js:babel-register --recursive --require jsdom-global/register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ioof-holdings/redux-dynamic-reducer.git"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.2.1",
    "lodash.isplainobject": "^4.0.6",
    "prop-types": "^15.5.0",
    "react-redux-subspace": "^2.0.6",
    "recompose": "^0.25.1",
    "redux-subspace": "^2.0.6"
  },
  "peerDependencies": {
    "react": "^15.1 || ^16.0"
  },
  "devDependencies": {
    "@types/react": "^15.0.0",
    "babel-cli": "^6.18.0",
    "babel-core": " ^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^2.2.5",
    "nyc": "^10.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.4",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "typescript": "^2.1.5",
    "typescript-definition-tester": "0.0.5"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output",
    "reporter": [
      "html",
      "text",
      "json"
    ]
  }
}
